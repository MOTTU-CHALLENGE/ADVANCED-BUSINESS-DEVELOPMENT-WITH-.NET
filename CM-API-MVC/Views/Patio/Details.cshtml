@model Patio

@{
    ViewData["Title"] = "Detalhes do Patio";
}

<h2>@ViewData["Title"]</h2>


<div>
    <dl class="row">
        <dt class="col-sm-2">ID</dt>
        <dd class="col-sm-10">@Model.IdPatio</dd>

        <dt class="col-sm-2">Nome do Patio</dt>
        <dd class="col-sm-10">@Model.NomePatio</dd>

        <dt class="col-sm-2">Filial</dt>
        <dd class="col-sm-10">@Model.IdFilial</dd>

        <dt class="col-sm-2">Capacidade</dt>
        <dd class="col-sm-10">@Model.CapacidadeMax</dd>

        <dt class="col-sm-2">Area</dt>
        <dd class="col-sm-10">@Model.Area</dd>

        <dt class="col-sm-2">Descrição</dt>
        <dd class="col-sm-10">@Model.Descricao</dd>

    </dl>
</div>

<a asp-action="Index" class="btn btn-secondary">Voltar</a>
